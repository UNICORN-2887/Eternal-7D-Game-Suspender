Function 选角色
	
	Call Handle
	Call HP
	
	Delay 1000

	' 声明并初始化一维数组 char
	Dim char(2) ' 创建一个包含 3 个元素的数组（索引从 0 到 2）
	
	' 将数组的所有元素初始化为 0
	char(0) = -1
	char(1) = -1
	char(2) = -1 
	
	Do
	//定义公共遍历变量
	Dim u
	u = 0
	
	
	' 声明一个 6x7 的二维数组
	Dim Factor(5, 6)
	
	' 声明各个一维数组
	Dim A1, A2, B1, B2, C1, C2
	A1 = Array(0, 50, 443, 215, 588, 133, 305)
	A2 = Array(0, 259, 441, 426, 588, 347, 309)
	B1 = Array(0, 49, 633, 216, 801, 133, 530)
	B2 = Array(0, 261, 636, 426, 801, 340, 515)
	C1 = Array(0, 49, 845, 215, 1013, 133, 720)
	C2 = Array(0, 260, 845, 427, 1013, 343, 727)
	
	' 将一维数组的数据复制到二维数组中
	For i = 0 To 6
    	Factor(0, i) = A1(i)
    	Factor(1, i) = A2(i)
    	Factor(2, i) = B1(i)
    	Factor(3, i) = B2(i)
    	Factor(4, i) = C1(i)
    	Factor(5, i) = C2(i)
	Next
	
	' 访问二维数组中的元素
	//MsgBox Factor(0, 1)' 输出 50
	Dim j
	j = 0
		Do
		XY支援2 = Plugin.Bkgnd.FindColor(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "DEDEFF")//是否被禁用
		ZB支援2 = InStr(XY支援2, "|")
		X支援2 = Clng(Left(XY支援2, ZB支援2 - 1)) : Y支援2 = Clng(Right(XY支援2, Len(XY支援2) - ZB支援2))
		
		XY支援5 = Plugin.Bkgnd.FindColor(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "DFDFFF")//是否被禁用
		ZB支援5 = InStr(XY支援5, "|")
		X支援5 = Clng(Left(XY支援5, ZB支援5 - 1)) : Y支援5 = Clng(Right(XY支援5, Len(XY支援5) - ZB支援5))
		
		XY支援6 = Plugin.Bkgnd.FindColor(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "DDDDFF")//是否被禁用
		ZB支援6 = InStr(XY支援6, "|")
		X支援6 = Clng(Left(XY支援6, ZB支援6 - 1)) : Y支援6 = Clng(Right(XY支援6, Len(XY支援6) - ZB支援6))
		If X支援2 <> - 1 and X支援5 <> - 1 and X支援6 <> - 1 Then //被禁用
			//将第一位价值赋值为-1 禁用
			Factor(j, 0) = -1
			MsgBox "禁用"
		Else //可用 判断buff
			XY支援3 = Plugin.Bkgnd.FindColorEx(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "A1ECFC", 0, 0.98)//是否有buff//
			ZB支援3 = InStr(XY支援3, "|")
			X支援3 = Clng(Left(XY支援3, ZB支援3 - 1)) : Y支援3 = Clng(Right(XY支援3, Len(XY支援3) - ZB支援3))
			
			XY支援4 = Plugin.Bkgnd.FindColorEx(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "9FEAF9", 0, 0.99)//是否有buff//
			ZB支援4 = InStr(XY支援4, "|")
			X支援4 = Clng(Left(XY支援4, ZB支援4 - 1)) : Y支援4 = Clng(Right(XY支援4, Len(XY支援4) - ZB支援4))
			
			XY支援7 = Plugin.Bkgnd.FindColorEx(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "4F75B5", 0, 0.93)//是否有buff//
			ZB支援7 = InStr(XY支援7, "|")
			X支援7 = Clng(Left(XY支援7, ZB支援7 - 1)) : Y支援7 = Clng(Right(XY支援7, Len(XY支援7) - ZB支援7))
			
			XY支援8 = Plugin.Bkgnd.FindPic(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "D:\工程\项目图片\7d\支援加成.bmp", 0, 0.7)
			iZB支援8 = InStr(XY支援8, "|")
			X支援8 = CLng(Left(XY支援8, iZB支援8 - 1))
			Y支援8 = CLng(Right(XY支援8, Len(XY支援8) - iZB支援8))

//			XY支援8 = Plugin.Bkgnd.FindColorEx(HD1, Factor(j, 1), Factor(j, 2), Factor(j, 3), Factor(j, 4), "213DAE", 0, 0.95)//是否有buff//
//			ZB支援8 = InStr(XY支援8, "|")
//			X支援8 = Clng(Left(XY支援8, ZB支援8 - 1)) : Y支援8 = Clng(Right(XY支援8, Len(XY支援8) - ZB支援8))
			If X支援3 <> - 1 and X支援4 <> - 1 and X支援7 <> - 1 and X支援8 <> - 1 Then //有buff
				//将第一位价值赋值为+1 备选
				Factor(j, 0) = 1
				MsgBox "加成"
			Else 
				MsgBox "自然"
			End If
			
			
		End If
		
		j = j + 1
		Loop Until j = 6
		
		MsgBox Factor(0, 0)
		MsgBox Factor(1, 0)
		MsgBox Factor(2, 0)
		MsgBox Factor(3, 0)
		MsgBox Factor(4, 0)
		MsgBox Factor(5, 0)
	//选用角色
	//变量价值 如果有1直接调用 
	Dim k
	k = 0
		Do
		TracePrint "fac(0,0)" & Factor(k, 0)
		If Factor(k, 0) = 1 Then //先选buff 有buff （看第一元素价值判断）
		//把角色放入槽中
			If char(0) = - 1  Then //如果第一角色空缺
				Call Plugin.Bkgnd.LeftClick(HD2, Factor(k, 5), Factor(k, 6))//点击该角色
				char(0) = 1//将第一角色赋值
				Factor(k, 0) = - 1 //价值归零 防止被二次搜索
				MessageBox "p1 input"
			Else //如果第一角色满
				If char(1) = -1 Then //如果第二角色空缺
					Call Plugin.Bkgnd.LeftClick(HD2, Factor(k, 5), Factor(k, 6))//点击该角色
					char(1) = 1//将第二角色赋值
					Factor(k, 0) = - 1 //价值归零 防止被二次搜索
					MessageBox "p1 input"
				Else //如果第二角色满
					If char(2) = - 1  Then //如果第三角色空缺
						Call Plugin.Bkgnd.LeftClick(HD2, Factor(k, 5), Factor(k, 6))//点击该角色
						char(2) = 1//将第三角色赋值
						Factor(k, 0) = - 1 //价值归零 防止被二次搜索
						MessageBox "p1 input"
					Else 
						Goto out
					End If
				End If
			End If
		Else 
		
		End If
		k = k + 1
		Loop Until k = 6
	
	//看看
	If char(2) = - 1  Then //如果第三角色空缺
	//MessageBox "ct vacancy"
	Else 
		Goto out
	End If
	//MessageBox "p2"
	//重置k 遍历0级
	Dim q
	q = 0
		Do
		TracePrint "factor (0.0)" & Factor(q, 0)
		If Factor(q, 0) = 0 Then //无buff也选（看第一元素价值判断）
		//把角色放入槽中
			If char(0) = - 1  Then //如果第一角色空缺
				Call Plugin.Bkgnd.LeftClick(HD2, Factor(q, 5), Factor(q, 6))//点击该角色
				char(0) = 1//将第一角色赋值
				Factor(q, 0) = - 1 //价值归零 防止被二次搜索
				MessageBox "p2 input"
			Else //如果第一角色满
				If char(1) = -1 Then //如果第二角色空缺
					Call Plugin.Bkgnd.LeftClick(HD2, Factor(q, 5), Factor(q, 6))//点击该角色
					char(1) = 1//将第二角色赋值
					Factor(q, 0) = - 1 //价值归零 防止被二次搜索
					MessageBox "p2 input"
				Else //如果第二角色满
					If char(2) = - 1  Then //如果第三角色空缺
						Call Plugin.Bkgnd.LeftClick(HD2, Factor(q, 5), Factor(q, 6))//点击该角色
						char(2) = 1//将第三角色赋值
						Factor(q, 0) = - 1 //价值归零 防止被二次搜索
						MessageBox "p2 input"
					Else 
						Goto out
					End If
				End If
			End If
		Else 
			//MsgBox "ps skip"
		End If
		q = q + 1
		Loop Until q = 6
	//都没有搜到
	//往下翻
	u = u + 1
	Delay 500
	//MsgBox "frag now"
	Call 滑动(0, - 50 , 550, - 50 , 0, 1000)//下滑
	Delay 3000
	
	Loop Until u = 4//下滑三次
	Rem out
End Function

